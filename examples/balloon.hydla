INIT <=> h=10&h'=0&break=0&timer=0&fix=3&interval>3&interval<10.
CONST <=> [](fix' = 0 & interval' = 0).

TIMER <=> [](timer' = 1).
FLOAT <=> [](break = 0 => h'' = 1).
SINK <=> [](break = 1 => h'' = -2).

BREAK <=> [](timer->=interval => break=1).
NORMAL <=> [](timer-<interval => break=0).

RESET <=> [](timer- >= interval + fix => timer = 0). 

ASSERT(h >= 0).

INIT, CONST, FLOAT, SINK, NORMAL, BREAK, TIMER << RESET.
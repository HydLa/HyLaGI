//2段式燃料ロケットによる人工衛星の打ち上げ

INIT<=>m2=1000/(k+1)/\m1=m2*k
       /\[](c=3 /\ e=8/10 /\ P=10)
       /\m=m1+m2+P/\v=0/\sw=0/\k=20.
//       /\m=m1+m2+P/\v=0/\sw=0/\k<=10/\k>=1.
CONST<=>[]( m2'=0 /\ m1'=0 /\ k'=0).
SW<=>[](sw'=0).

JET<=>[](sw-=0 => v'=-c/m*m' /\ m'=-1 ).

ONE<=>[]( m-=(1-e)*m1+m2+P => m=m2+P ).
TWO<=>[]( m-=(1-e)*m2+P => sw=1).

FIN<=>[](sw-=1 => v'=0 /\ m'=0).

INIT, CONST, FIN, JET<<SW<<ONE<<TWO.
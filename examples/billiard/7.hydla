INIT(x,x0,vx0) <=> x=x0 & x'=vx0.
COL(x1,x2) <=> [](x1-=x2- => x1'=x2'- & x2'=x1'-).
CONST(x) <=> [](x''=0).
WALL(x) <=> []((x-=-10 | x-=1000) => x' = -x'-).

INIT(x0,0,1),INIT(x1,1,0),INIT(x2,4,0),INIT(x3,5,0),INIT(x4,6,0),INIT(x5,10,0),INIT(x6,11,0),
WALL(x0),WALL(x1),WALL(x2),WALL(x3),WALL(x4),WALL(x5),WALL(x6),
(CONST(x0),CONST(x1),CONST(x2),CONST(x3),CONST(x4),CONST(x5),CONST(x6))
 << (COL(x0,x1),COL(x0,x2),COL(x0,x3),COL(x0,x4),COL(x0,x5),COL(x0,x6),COL(x1,x2),COL(x1,x3),COL(x1,x4),COL(x1,x5),COL(x1,x6),COL(x2,x3),COL(x2,x4),COL(x2,x5),COL(x2,x6),COL(x3,x4),COL(x3,x5),COL(x3,x6),COL(x4,x5),COL(x4,x6),COL(x5,x6)).

INIT(x,x0,vx0) <=> x=x0 & x'=vx0.
COL(x1,x2) <=> [](x1-=x2- => x1'=x2'- & x2'=x1'-).
CONST(x) <=> [](x''=0).
WALL(x) <=> [](x-=-1 | x-=40 => x'=-x'-).

INIT(x0,0,1),INIT(x1,3,0),INIT(x2,9,0),INIT(x3,15,0),
(CONST(x0),CONST(x1),CONST(x2),CONST(x3)) << 
(WALL(x0),WALL(x1),WALL(x2),WALL(x3)
,COL(x0,x1),COL(x0,x2),COL(x0,x3)
,COL(x1,x2),COL(x1,x3)
,COL(x2,x3)
).
